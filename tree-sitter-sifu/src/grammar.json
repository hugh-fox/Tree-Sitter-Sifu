    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+(\\.[0-9]+)?"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[:!@$%^&*+-=|<>?\\/\\\\~`\\p{S}]+",
      "flags": "u"
    },
    "nested_pattern": {
