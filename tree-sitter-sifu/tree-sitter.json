{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sifu",
      "camelcase": "Sifu",
      "title": "Sifu",
      "scope": "source.sifu",
      "file-types": [
        "sifu"
      ],
      "injection-regex": "^sifu$",
      "class-name": "TreeSitterSifu"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache 2.0",
    "description": "The Sifu Programming Language Parser",
    "authors": [
      {
        "name": "Hugh Fox",
        "email": "hugh.s.fox@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/hugh-fox/tree-sitter-sifu"
    }
  },
  "bindings": {
    "c": true
  }
}